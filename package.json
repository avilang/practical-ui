{
  "name": "@avilang/practical-ui",
  "description": "基于 React 的后台 UI 库",
  "version": "0.1.3",
  "scripts": {
    "build": "rimraf dist && rollup -c && rimraf dist/index.css",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern lib/_fonts/* --ext .js lib",
    "lint:style": "stylelint --ignore-path .gitignore --fix lib/**/*.scss",
    "docz:dev": "rimraf .docz && docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --ignore-path .gitignore --ignore-pattern lib/_fonts/* --ignore-pattern dist/*",
    "*.scss": "stylelint --cache --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "docz": "^2.3.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-sass": "^4.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.15",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-postcss": "^4.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0"
  },
  "engines": {
    "node": ">=14.16.1 <15"
  },
  "license": "MIT"
}
