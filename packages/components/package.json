{
  "name": "@avilang/practical-ui",
  "type": "module",
  "version": "0.2.0",
  "description": "Practical UI components created based on vue3",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "format": "prettier . --write",
    "prepare": "cd ../.. && husky packages/components/.husky"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint",
      "prettier --check"
    ],
    "*.{css,less}": "prettier --check"
  },
  "keywords": [
    "naive-ui",
    "vue3",
    "practical ui"
  ],
  "peerDependencies": {
    "vue": "^3.5.10"
  },
  "dependencies": {
    "grapheme-splitter": "1.0.4",
    "naive-ui": "2.40.1",
    "vfonts": "0.0.3"
  },
  "devDependencies": {
    "@laynezh/vite-plugin-lib-assets": "^0.5.24",
    "@vitejs/plugin-vue": "^5.1.4",
    "vite": "^5.4.8"
  },
  "author": "avilang <1985945885@qq.com> (https://github.com/avilang)",
  "license": "MIT"
}
